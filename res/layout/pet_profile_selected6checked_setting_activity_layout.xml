<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ipetchat="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_light_gray"
    android:paddingBottom="20dp"
    android:paddingLeft="12dp"
    android:paddingRight="12dp"
    android:paddingTop="20dp"
    android:scrollbars="none" >

    <com.segotech.ipetchat.customwidget.PetProfileSelected6CheckedSettingListView
        android:id="@+id/pet_profile_selected6checked_setting_listView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/pet_profile_selected6checked_setting_listview_bg"
        android:cacheColorHint="@android:color/transparent"
        android:divider="@color/middle_gray"
        android:dividerHeight="1dp"
        android:fadingEdge="none"
        android:headerDividersEnabled="false"
        android:listSelector="@android:color/transparent"
        android:scrollbars="none" >
    </com.segotech.ipetchat.customwidget.PetProfileSelected6CheckedSettingListView>

</FrameLayout>
